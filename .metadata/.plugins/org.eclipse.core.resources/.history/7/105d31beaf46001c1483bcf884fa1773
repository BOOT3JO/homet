package com.homet.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

@RequestMapping("/mealkit")
@Controller
public class MealkitController {

	@RequestMapping(value = "/")
	public String mealkithome() {
		return "mealkit/mealkithome";
	}
	
	@RequestMapping(value = "/mealkit")
	public String makekit() {
		return "mealkit/mealkit";
	}
	
	@RequestMapping(value = "/mealkitset")
	public String setList() {
		return "mealkit/mealkitset";
	}
	
	
	
}
